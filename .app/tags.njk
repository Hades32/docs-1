---
title: Tags
layout: base
permalink: "/tags/"
---

<h1>Tags</h1>
{% for tag in collections.tags %}
  <div class="tag">
    <h2>{{ tag }}</h2>
    <ul>
      {% for post in collections[tag] %}
        <li><a href="{{ post.url }}">{{ post.data.title or post.page.fileSlug }}</a></li>
      {% endfor %}
    </ul>
  </div>
{% endfor %}

