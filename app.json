{
  "$schema": ".app/app.schema.json",
  "title": "Eleventy Notes",
  "description": "A simple, lightweight, and flexible note-taking template for Eleventy.",
  "editThisNote": {
    "url": "https://github.com/rothsandro/eleventy-notes/edit/{{branch}}/{{file}}"
  },
  "sidebar": {
    "links": [
      {
        "url": "https://github.com/rothsandro/eleventy-notes/issues",
        "label": "Request feature",
        "icon": "gift"
      },
      {
        "url": "https://github.com/rothsandro/eleventy-notes",
        "label": "GitHub",
        "icon": "github"
      }
    ],
    "notes": [
      {
        "pattern": "^/[^/]+$",
        "label": ""
      },
      {
        "pattern": "^/Configuration/",
        "label": "Configuration"
      },
      {
        "pattern": "^/Features/",
        "label": "Features"
      },
      {
        "pattern": "^/Deployment/",
        "label": "Deployment"
      }
    ]
  }
}
